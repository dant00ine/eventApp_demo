<h3>Index of all Users</h3>
<table class="table table-striped">
  <tr>
    <th>name</th>
    <th>email</th>
    <th>age</th>
    <th>Profile</th>
  </tr>
<% @allUsers.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.age %></td>
    <td><a href="/users/<%= user.id %>">View </a></td>
  </tr>
<% end %>
</table>
