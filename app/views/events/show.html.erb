<h3><%= @event.title %></h3>
<p><%= @event.location %></p>

<table class="table">
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
      <th>age</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
        <td><%= member.age %></td>
        <td><a href="/users/<%= member.id %>">View</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
